{
  "name": "ts-starter",
  "version": "1.0.0",
  "packageManager": "pnpm@7.12.2",
  "description": "typescript-template",
  "author": "peterroe",
  "license": "MIT",
  "keywords": [
    "typescript",
    "template"
  ],
  "homepage": "https://github.com/peterroe/un#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/peterroe/un.git"
  },
  "bugs": "https://github.com/peterroe/un/issues",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./*": "./*"
  },
  "scripts": {
    "build": "unbuild",
    "lint": "eslint \"**/*.{ts,js}\"",
    "fix": "eslint --fix \"**/*.{ts,js}\"",
    "start": "esno src/index.ts",
    "test": "vitest",
    "release": "bumpp && npm publish",
    "typecheck": "tsc --noEmit",
    "git-hook-init": "husky install && husky add .husky/pre-commit 'npm run lint && npm run typecheck'"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "devDependencies": {
    "@antfu/eslint-config": "0.27.0",
    "bumpp": "8.2.1",
    "eslint": "8.24.0",
    "esno": "0.16.3",
    "husky": "8.0.1",
    "typescript": "4.8.4",
    "unbuild": "0.8.11",
    "vite": "3.1.5",
    "vitest": "0.23.4"
  }
}